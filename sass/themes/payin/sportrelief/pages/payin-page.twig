<header class="header" role="banner">
  <div class="header__inner-wrapper">
    <div id="block-branding" class="block block-branding">
      <a href="https://www.sportrelief.com" target="_blank" title="Home" rel="home" class="site-logo">
        <img src="/images/sr18_logo_date_blacktext.svg" alt="Home">
      </a>
    </div>
    <div class="header__inner-home">
      <a href="https://www.sportrelief.com/" title="Back to the Sport Relief homepage" class="link inline">
      <span>< Back to home</span>
      </a>
    </div>
  </div>
</header>
<main role="contentinfo">
  <div class="promo-header promo-header--small promo-header--success">
    <div class="promo-header__content">
      <div class="promo-header__content-inner promo-header__content-inner--centre">
        <h1>Pay in online</h1>
        <p>
          Paying in your fundraising money online is quick and easy.<br />
          Firstly, select who you are paying in for...
        </p>
      </div>
    </div>
  </div>
  <section id="payin-step-one" class="cards cards-wall bg--black cards--four-grid-centre js-showhide-options">
    <div class="cards__wrapper">
      <!-- Myself, friends or family -->
      <div class="card card--family">
        <a id="audience__public" href="#payin-step-two" aria-label="paying in money from fundraising by yourself or with friends and family.">
          <div class="cards__content">
            <h3>Myself, friends or family</h3>
          </div>
        </a>
      </div>
      <!-- Nursery, school or university -->
      <div class="card card--school">
        <a id="audience__school" href="#payin-step-two" aria-label="paying in money from fundraising in a nursery, school, university or youth group.">
          <div class="cards__content">
            <h3>Nursery, School, Youth Group or University</h3>
          </div>
        </a>
      </div>
      <!-- Workplace or organisation -->
      <div class="card card--work">
        <a id="audience__work" href="#payin-step-two" aria-label="paying in money from fundraising in a workplace.">
          <div class="cards__content">
            <h3>Workplace or Organisation</h3>
          </div>
        </a>
      </div>
      <!-- Gym, Leisure Centre, or Sports Club -->
      <div class="card card--leisure">
        <a id="audience__leisure" href="#payin-step-two" aria-label="paying in money from fundraising in a gym, leisure centre or with a sports club.">
          <div class="cards__content">
            <h3>Gym, Leisure Centre, or Sports Club</h3>
          </div>
        </a>
      </div>
    </div>
  </section>
